&Тест
Процедура ТестВложенныхПустыхКоллекций() Экспорт
    // Дано
    СтрокаYAML = 
    "parent:
    |  empty_array: []
    |  empty_object: {}
    |  null_value: null
    |  empty_value:";
    СтрокаYAML = СтрЗаменить(СтрокаYAML, "|", "");
    ЧтениеYaml = ТестовыеУтилиты.СоздатьЭкземплярПарсера();
    
    // Когда
    Результат = ЧтениеYaml.Прочитать(СтрокаYAML);
    
    // Тогда
    Ожидаем.Что(Результат).Не_().ЭтоНеопределено();
    Родитель = Результат.Получить("parent");
    Ожидаем.Что(Родитель).Не_().ЭтоНеопределено();
    Ожидаем.Что(ТипЗнч(Родитель)).Равно(Тип("Соответствие"));
    
    // Проверяем пустой массив
    ПустойМассив = Родитель.Получить("empty_array");
    Ожидаем.Что(ТипЗнч(ПустойМассив)).Равно(Тип("Массив"));
    Ожидаем.Что(ПустойМассив.Количество()).Равно(0);
    
    // Проверяем пустой объект
    ПустойОбъект = Родитель.Получить("empty_object");
    Ожидаем.Что(ТипЗнч(ПустойОбъект)).Равно(Тип("Соответствие"));
    Ожидаем.Что(ПустойОбъект.Количество()).Равно(0);
    
    // Проверяем null значение
    НullЗначение = Родитель.Получить("null_value");
    Ожидаем.Что(НullЗначение).ЭтоНеопределено();
    
    // Проверяем пустое значение
    ПустоеЗначение = Родитель.Получить("empty_value");
    Ожидаем.Что(ПустоеЗначение).ЭтоНеопределено();
КонецПроцедуры

&Тест
Процедура ТестКомбинацииПустыхИНепустыхКоллекций() Экспорт
    // Дано
    СтрокаYAML = 
    "mixed:
    |  empty_list: []
    |  filled_list: [1, 2, 3]
    |  empty_dict: {}
    |  filled_dict: {key: value}
    |  null_list: null
    |  implicit_empty:";
    СтрокаYAML = СтрЗаменить(СтрокаYAML, "|", "");
    ЧтениеYaml = ТестовыеУтилиты.СоздатьЭкземплярПарсера();
    
    // Когда
    Результат = ЧтениеYaml.Прочитать(СтрокаYAML);
    
    // Тогда
    Ожидаем.Что(Результат).Не_().ЭтоНеопределено();
    Смешанное = Результат.Получить("mixed");
    Ожидаем.Что(Смешанное).Не_().ЭтоНеопределено();
    
    // Пустой список
    ПустойСписок = Смешанное.Получить("empty_list");
    Ожидаем.Что(ТипЗнч(ПустойСписок)).Равно(Тип("Массив"));
    Ожидаем.Что(ПустойСписок.Количество()).Равно(0);
    
    // Заполненный список
    ЗаполненныйСписок = Смешанное.Получить("filled_list");
    Ожидаем.Что(ТипЗнч(ЗаполненныйСписок)).Равно(Тип("Массив"));
    Ожидаем.Что(ЗаполненныйСписок.Количество()).Равно(3);
    
    // Пустой словарь
    ПустойСловарь = Смешанное.Получить("empty_dict");
    Ожидаем.Что(ТипЗнч(ПустойСловарь)).Равно(Тип("Соответствие"));
    Ожидаем.Что(ПустойСловарь.Количество()).Равно(0);
    
    // Заполненный словарь
    ЗаполненныйСловарь = Смешанное.Получить("filled_dict");
    Ожидаем.Что(ТипЗнч(ЗаполненныйСловарь)).Равно(Тип("Соответствие"));
    Ожидаем.Что(ЗаполненныйСловарь.Количество()).Равно(1);
    
    // Null список
    НullСписок = Смешанное.Получить("null_list");
    Ожидаем.Что(НullСписок).ЭтоНеопределено();
    
    // Неявно пустое значение
    НеявноПустое = Смешанное.Получить("implicit_empty");
    Ожидаем.Что(НеявноПустое).ЭтоНеопределено();
КонецПроцедуры

&Тест
Процедура ТестКомплексныхПустыхКоллекций() Экспорт
    // Дано
    СодержимоеФайла = ТестовыеУтилиты.ПрочитатьТекстФайла(ТестовыеУтилиты.ПолучитьПутьКТестовымДанным("empty_collections.yaml"));
    ЧтениеYaml = ТестовыеУтилиты.СоздатьЭкземплярПарсера();
    
    // Когда
    Результат = ЧтениеYaml.Прочитать(СодержимоеФайла);
    
    // Тогда
    Ожидаем.Что(Результат).Не_().ЭтоНеопределено();
    
    // Проверяем простые пустые коллекции
    ПустойМассив = Результат.Получить("empty_array");
    Ожидаем.Что(ТипЗнч(ПустойМассив)).Равно(Тип("Массив"));
    Ожидаем.Что(ПустойМассив.Количество()).Равно(0);
    
    ПустойОбъект = Результат.Получить("empty_object");
    Ожидаем.Что(ТипЗнч(ПустойОбъект)).Равно(Тип("Соответствие"));
    Ожидаем.Что(ПустойОбъект.Количество()).Равно(0);
    
    // Проверяем пустые значения
    НullЗначение = Результат.Получить("null_value");
    Ожидаем.Что(НullЗначение).ЭтоНеопределено();
    
    ТильдаЗначение = Результат.Получить("tilde_value");
    Ожидаем.Что(ТильдаЗначение).ЭтоНеопределено();
    
    НеявноПустое = Результат.Получить("implicit_empty");
    Ожидаем.Что(НеявноПустое).ЭтоНеопределено();
    
    // Проверяем вложенные пустые коллекции
    ВложенныеПустые = Результат.Получить("nested_empty");
    Ожидаем.Что(ВложенныеПустые).Не_().ЭтоНеопределено();
    
    МассивыВложенных = ВложенныеПустые.Получить("arrays");
    Ожидаем.Что(ТипЗнч(МассивыВложенных)).Равно(Тип("Массив"));
    Ожидаем.Что(МассивыВложенных.Количество()).Равно(3);
    
    // Первый элемент - пустой массив
    ПервыйЭлемент = МассивыВложенных[0];
    Ожидаем.Что(ТипЗнч(ПервыйЭлемент)).Равно(Тип("Массив"));
    Ожидаем.Что(ПервыйЭлемент.Количество()).Равно(0);
    
    // Второй элемент - заполненный массив
    ВторойЭлемент = МассивыВложенных[1];
    Ожидаем.Что(ТипЗнч(ВторойЭлемент)).Равно(Тип("Массив"));
    Ожидаем.Что(ВторойЭлемент.Количество()).Равно(3);
    
    // Третий элемент - пустой массив
    ТретийЭлемент = МассивыВложенных[2];
    Ожидаем.Что(ТипЗнч(ТретийЭлемент)).Равно(Тип("Массив"));
    Ожидаем.Что(ТретийЭлемент.Количество()).Равно(0);
КонецПроцедуры

