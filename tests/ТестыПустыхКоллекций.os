&Тест
Процедура ТестДемонстрацииПустыхКоллекций() Экспорт
    // Дано
    СодержимоеФайла = ТестовыеУтилиты.ПрочитатьТекстФайла(ТестовыеУтилиты.ПолучитьПутьКТестовымДанным("demo_empty.yaml"));
    ЧтениеYaml = ТестовыеУтилиты.СоздатьЭкземплярПарсера();
    
    // Когда
    Результат = ЧтениеYaml.Прочитать(СодержимоеФайла);
    
    // Тогда
    Ожидаем.Что(Результат).Не_().ЭтоНеопределено();
    
    // Flow Style пустые коллекции
    ПустойFlowМассив = Результат.Получить("empty_flow_array");
    Ожидаем.Что(ТипЗнч(ПустойFlowМассив)).Равно(Тип("Массив"));
    Ожидаем.Что(ПустойFlowМассив.Количество()).Равно(0);
    
    ПустойFlowОбъект = Результат.Получить("empty_flow_object");
    Ожидаем.Что(ТипЗнч(ПустойFlowОбъект)).Равно(Тип("Соответствие"));
    Ожидаем.Что(ПустойFlowОбъект.Количество()).Равно(0);
    
    // Block Style пустые коллекции (неявные)
    ПустойBlockМассив = Результат.Получить("empty_block_array");
    Ожидаем.Что(ПустойBlockМассив).ЭтоНеопределено();
    
    ПустойBlockОбъект = Результат.Получить("empty_block_object");
    Ожидаем.Что(ПустойBlockОбъект).ЭтоНеопределено();
    
    // Явные null значения
    НullМассив = Результат.Получить("null_array");
    Ожидаем.Что(НullМассив).ЭтоНеопределено();
    
    НullОбъект = Результат.Получить("null_object");
    Ожидаем.Что(НullОбъект).ЭтоНеопределено();
    
    ТильдаМассив = Результат.Получить("tilde_array");
    Ожидаем.Что(ТильдаМассив).ЭтоНеопределено();
    
    ТильдаОбъект = Результат.Получить("tilde_object");
    Ожидаем.Что(ТильдаОбъект).ЭтоНеопределено();
    
    // Смешанные случаи
    СмешаннаяКоллекция = Результат.Получить("mixed_collection");
    Ожидаем.Что(СмешаннаяКоллекция).Не_().ЭтоНеопределено();
    
    // Заполненный массив
    ЗаполненныйМассив = СмешаннаяКоллекция.Получить("filled_array");
    Ожидаем.Что(ТипЗнч(ЗаполненныйМассив)).Равно(Тип("Массив"));
    Ожидаем.Что(ЗаполненныйМассив.Количество()).Равно(3);
    
    // Пустой массив
    ПустойМассив = СмешаннаяКоллекция.Получить("empty_array");
    Ожидаем.Что(ТипЗнч(ПустойМассив)).Равно(Тип("Массив"));
    Ожидаем.Что(ПустойМассив.Количество()).Равно(0);
    
    // Заполненный объект
    ЗаполненныйОбъект = СмешаннаяКоллекция.Получить("filled_object");
    Ожидаем.Что(ТипЗнч(ЗаполненныйОбъект)).Равно(Тип("Соответствие"));
    Ожидаем.Что(ЗаполненныйОбъект.Количество()).Равно(2);
    Ожидаем.Что(ЗаполненныйОбъект.Получить("name")).Равно("test");
    Ожидаем.Что(ЗаполненныйОбъект.Получить("value")).Равно(123);
    
    // Пустой объект
    ПустойОбъект = СмешаннаяКоллекция.Получить("empty_object");
    Ожидаем.Что(ТипЗнч(ПустойОбъект)).Равно(Тип("Соответствие"));
    Ожидаем.Что(ПустойОбъект.Количество()).Равно(0);
    
    // Null поле
    НullПоле = СмешаннаяКоллекция.Получить("null_field");
    Ожидаем.Что(НullПоле).ЭтоНеопределено();
    
    // Неявно пустое поле
    НеявноПустое = СмешаннаяКоллекция.Получить("implicit_empty");
    Ожидаем.Что(НеявноПустое).ЭтоНеопределено();
КонецПроцедуры
