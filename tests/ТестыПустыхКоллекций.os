&Тест
Процедура ТестПустыхКоллекцийВBlockStyle() Экспорт
    // Дано
    СтрокаYAML = 
    "config:
    |  database:
    |    host: localhost
    |    port: 5432
    |    credentials:
    |    settings:
    |      timeout: 30
    |      retries:
    |  application:
    |    debug: true
    |    features:
    |    logging:
    |      level: info
    |      handlers:";
    СтрокаYAML = СтрЗаменить(СтрокаYAML, "|", "");
    ЧтениеYaml = ТестовыеУтилиты.СоздатьЭкземплярПарсера();
    
    // Когда
    Результат = ЧтениеYaml.Прочитать(СтрокаYAML);
    
    // Тогда
    Ожидаем.Что(Результат).Не_().ЭтоНеопределено();
    
    Конфигурация = Результат.Получить("config");
    Ожидаем.Что(Конфигурация).Не_().ЭтоНеопределено();
    
    // Проверяем базу данных
    БД = Конфигурация.Получить("database");
    Ожидаем.Что(БД).Не_().ЭтоНеопределено();
    Ожидаем.Что(БД.Получить("host")).Равно("localhost");
    Ожидаем.Что(БД.Получить("port")).Равно(5432);
    
    // Проверяем пустые поля в базе данных
    Учетные = БД.Получить("credentials");
    Ожидаем.Что(Учетные).ЭтоНеопределено();
    
    Настройки = БД.Получить("settings");
    Ожидаем.Что(Настройки).Не_().ЭтоНеопределено();
    Ожидаем.Что(Настройки.Получить("timeout")).Равно(30);
    
    Попытки = Настройки.Получить("retries");
    Ожидаем.Что(Попытки).ЭтоНеопределено();
    
    // Проверяем приложение
    Приложение = Конфигурация.Получить("application");
    Ожидаем.Что(Приложение).Не_().ЭтоНеопределено();
    Ожидаем.Что(Приложение.Получить("debug")).Равно(Истина);
    
    // Проверяем пустые поля в приложении
